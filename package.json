{
  "name": "my-app",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "predev": "tsx scripts/manage-openapi.ts",
    "dev": "concurrently \"zudoku dev\" \"tsx server.ts\"",
