{
  "buildCommand": "npm run build",
  "outputDirectory": "./dist",
  "framework": null,
  "redirects": [
    {
      "source": "/",
      "destination": "/introduction",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/((?!\\.html).*)",
      "destination": "/index.html"
    }
  ]
}
