{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/introduction(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*\\..*)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "ZUDOKU_PUBLIC_BACKEND_URL": "https://zudoku-backend.onrender.com"
  }
}
