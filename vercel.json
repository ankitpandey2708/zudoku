{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/",
      "destination": "/introduction"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://zudoku-backend.onrender.com/api/:path*"
    }
  ]
}
